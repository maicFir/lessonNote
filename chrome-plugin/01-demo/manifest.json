{
    "manifest_version": 3,
    "name": "Maic_test_chrome",
    "description": "lesson demo",
    "version": "1.0",
    "author": "Maic",
    "action": {
        "default_popup": "popup/index.html",
        "default_icon": "logo.png"
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "css": [],
            "matches": [
                "https://*.wmcweb.cn/*",
                "<all_urls>"
            ]
        }
    ]
}